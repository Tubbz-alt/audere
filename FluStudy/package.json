{
  "name": "FluAtHome",
  "version": "0.2.1",
  "private": true,
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/expo": "^31.0.1",
    "@types/jest": "^23.3.2",
    "@types/pouchdb": "^6.3.2",
    "@types/react": "^16.4.14",
    "@types/react-native": "^0.57.18",
    "@types/react-navigation": "^2.0.24",
    "@types/react-test-renderer": "^16.0.2",
    "@types/url-parse": "^1.4.1",
    "@types/uuid": "^3.4.4",
    "babel-preset-expo": "^5.0.0",
    "jest-expo": "^31.0.0",
    "pouchdb": "^7.0.0",
    "prettier": "^1.14.3",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.1",
    "subtractiontype.ts": "^1.0.0",
    "ts-jest": "^23.1.4",
    "ts-mockito": "^2.3.1",
    "typescript": "^3.0.3"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "../scripts/build-hacks; ../scripts/generate-build-info; expo start",
    "start-localapi": "REACT_NATIVE_USE_LOCAL_SERVER=1 yarn run start",
    "stop": "expo stop",
    "eject": "expo eject",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test": "jest",
    "lint": "prettier --list-different *.{j,t} src/**/**/**/*.ts{,x}",
    "lint-fix": "prettier --write *.{j,t} src/**/*.ts{,x}; prettier --write *.{j,t} App.tsx; prettier --write src/**/**/*.ts{,x}",
    "typecheck": "tsc --noEmit"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "jest": {
    "preset": "jest-expo",
    "setupTestFrameworkScriptFile": "./test/setupJest",
    "setupFiles": [
      "./jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(js)$": "babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-|pouchdb-)?react-native|(@)?expo|(@)?react-navigation|pouchdb-adapter-asyncstorage))"
    ]
  },
  "dependencies": {
    "@types/buffer-xor": "^2.0.0",
    "@types/expo-localization": "^1.0.0",
    "@types/expo__vector-icons": "^6.2.3",
    "@types/i18next": "^11.9.3",
    "audere-lib": "link:../lib",
    "axios": "^0.18.0",
    "babel-plugin-inline-dotenv": "^1.2.0",
    "base64url": "^3.0.1",
    "buffer-xor": "^2.0.2",
    "crypto-pouch": "^3.1.3",
    "date-fns": "^1.29.0",
    "events": "^3.0.0",
    "expo": "^31.0.6",
    "expo-font": "^1.1.0",
    "expo-localization": "^1.0.0",
    "expo-pixi": "^0.3.1",
    "expokit": "31.0.2",
    "i18next": "^11.10.0",
    "immutable": "^4.0.0-rc.12",
    "node-libs-react-native": "^1.0.3",
    "pouchdb-react-native": "^6.4.1",
    "react": "16.5.0",
    "react-i18next": "^8.1.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-check-box": "^2.1.7",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^0.26.4",
    "react-native-fbsdk": "^0.8.0",
    "react-native-firebase": "^5.2.3",
    "react-native-get-random-values": "balderdash/react-native-get-random-values#v1.2.0-pre2",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-keyboard-listener": "^1.1.0",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-modal": "^7.0.0",
    "react-native-simple-radio-button": "^2.7.3",
    "react-navigation": "^3.2.1",
    "react-navigation-redux-helpers": "^3.0.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-encrypt": "^2.0.1",
    "redux-persist-transform-immutable": "^5.0.0",
    "snyk": "^1.136.0",
    "url-parse": "^1.4.4",
    "uuid": "^3.3.2"
  }
}
