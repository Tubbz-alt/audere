set -euo pipefail

REPO_ROOT_DIR="$(git rev-parse --show-toplevel)"
function updateAudereLib() {
  echo "Copying audere-lib"
  cp -r $REPO_ROOT_DIR/lib/ $REPO_ROOT_DIR/EbPhotoStore/node_modules/audere-lib
}
updateAudereLib
